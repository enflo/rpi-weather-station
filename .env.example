# Environment Configuration Example
# Copy this file to .env and fill in your values

# General Configuration
ENVIRONMENT=development
LOOP_TIME=1

# API Configuration
API_ENABLE=False
API_METHOD=POST
API_HOST=localhost
API_HEADER_TOKEN=your_token
API_URL_TOKEN=your_url_token

# MQTT Configuration
MQTT_ENABLE=False
MQTT_HOST=localhost
MQTT_PORT=1883
MQTT_TOPIC=weather
MQTT_USER=your_user
MQTT_PASSWORD=your_password

# SQS Configuration
SQS_ENABLE=False
SQS_URL=http://localhost:9324
SQS_ACCESS_KEY=your_access_key
SQS_SECRET_KEY=your_secret_key
SQS_QUEUE_NAME=your_queue_name
SQS_REGION=your_region

# Supabase Configuration (if needed)
SUPABASE_ENABLE=False
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_API_KEY=your_api_key
SUPABASE_TABLE=your_table

# PostgreSQL Configuration
POSTGRES_ENABLE=False
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_password
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DBNAME=postgres
POSTGRES_TABLE=weather_data